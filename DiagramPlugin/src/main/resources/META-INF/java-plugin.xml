<idea-plugin>
    <actions>
        <group>
            <add-to-group group-id="ProjectViewPopupMenu" anchor="last"/>
            <separator/>
            <action id="GenerateClassDiagramJava"
                    class="com.thesis.diagramplugin.plugin.actions.java.GenerateClassDiagramAction"
                    text="Generate Class Diagram of Java sources">
                <add-to-group group-id="ProjectViewPopupMenu" anchor="last"/>
            </action>
        </group>
        <group>
            <add-to-group group-id="EditorPopupMenu" anchor="last"/>
            <separator/>
            <action id="GenerateKopenogramJava"
                    class="com.thesis.diagramplugin.plugin.actions.java.GenerateKopenogramAction"
                    text="Generate Kopenogram of Java method">
            </action>
        </group>
    </actions>
</idea-plugin>